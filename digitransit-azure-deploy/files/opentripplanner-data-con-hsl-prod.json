{
  "acceptedResourceRoles": null,
  "backoffFactor": 1.15,
  "backoffSeconds": 1,
  "cmd": null,
  "constraints": [
    [
      "hostname",
      "UNIQUE"
    ]
  ],
  "container": {
    "docker": {
      "forcePullImage": true,
      "image": "hsldevcom/opentripplanner-data-container-hsl:prod",
      "network": "BRIDGE",
      "parameters": [
        {
          "key": "net",
          "value": "host"
        },
        {
          "key": "name",
          "value": "opentripplanner-data-con-hsl"
        }
      ],
      "portMappings": [
        {
          "containerPort": 8080,
          "hostPort": 0,
          "labels": {
            "VIP_0": "/opentripplanner-data-con-hsl:8080"
          },
          "protocol": "tcp",
          "servicePort": 10021
        }
      ],
      "privileged": false
    },
    "type": "DOCKER",
    "volumes": []
  },
  "cpus": 0.1,
  "dependencies": [],
  "disk": 0,
  "env": {},
  "gpus": 0,
  "healthChecks": [
    {
      "gracePeriodSeconds": 300,
      "ignoreHttp1xx": false,
      "intervalSeconds": 10,
      "maxConsecutiveFailures": 5,
      "path": "/version.txt",
      "portIndex": 0,
      "protocol": "HTTP",
      "timeoutSeconds": 10
    }
  ],
  "id": "/opentripplanner-data-con-hsl",
  "instances": 2,
  "labels": {
    "update": "auto"
  },
  "maxLaunchDelaySeconds": 3600,
  "mem": 128,
  "portDefinitions": [
    {
      "labels": {},
      "port": 10021,
      "protocol": "tcp"
    }
  ],
  "ports": [
    10021
  ],
  "readinessChecks": [],
  "taskKillGracePeriodSeconds": null,
  "upgradeStrategy": {
    "maximumOverCapacity": 1,
    "minimumHealthCapacity": 1
  }
}
