{
  "acceptedResourceRoles": null,
  "backoffFactor": 1.15,
  "backoffSeconds": 1,
  "cmd": null,
  "constraints": [
    [
      "hostname",
      "UNIQUE"
    ]
  ],
  "container": {
    "docker": {
      "forcePullImage": true,
      "image": "hsldevcom/digitransit-ui:index-page",
      "network": "HOST",
      "parameters": [],
      "portMappings": null,
      "privileged": false
    },
    "type": "DOCKER",
    "volumes": []
  },
  "cpus": 1,
  "dependencies": [],
  "disk": 0,
  "env": {
    "ASSET_URL": "https://digitransit-prod-cdn-origin.azureedge.net/ui/v1/hsl-next",
    "NODE_OPTS": "--max_old_space_size=600",
    "API_URL": "https://api.digitransit.fi",
    "PIWIK_ADDRESS": "//piwik.digitransit.fi/piwik.php",
    "RELAY_FETCH_TIMEOUT": "3000",
    "CONFIG": "hsl",
    "MAP_URL": "https://digitransit-prod-cdn-origin.azureedge.net",
    "OTP_URL":"https://api.digitransit.fi/routing/v1/routers/next-hsl/",
    "PIWIK_ID": "5"
  },
  "gpus": 0,
  "healthChecks": [
    {
      "gracePeriodSeconds": 300,
      "ignoreHttp1xx": false,
      "intervalSeconds": 10,
      "maxConsecutiveFailures": 3,
      "path": "/",
      "portIndex": 0,
      "protocol": "HTTP",
      "timeoutSeconds": 10
    }
  ],
  "id": "/digitransit-ui-hsl-next",
  "instances": 4,
  "labels": {
    "update": "auto"
  },
  "maxLaunchDelaySeconds": 3600,
  "mem": 800,
  "portDefinitions": [
    {
      "labels": {
        "VIP_0": "/digitransit-ui-hsl-next:8080"
      },
      "port": 8080,
      "protocol": "tcp"
    }
  ],
  "ports": [
    8080
  ],
  "readinessChecks": [],
  "taskKillGracePeriodSeconds": null,
  "upgradeStrategy": {
    "maximumOverCapacity": 1,
    "minimumHealthCapacity": 1
  }
}
