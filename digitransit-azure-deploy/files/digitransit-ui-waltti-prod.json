{
  "acceptedResourceRoles": null,
  "backoffFactor": 1.15,
  "backoffSeconds": 1,
  "cmd": null,
  "container": {
    "docker": {
      "forcePullImage": true,
      "image": "hsldevcom/digitransit-ui:prod",
      "network": "BRIDGE",
      "parameters": [
        {
          "key": "net",
          "value": "host"
        }
      ],
      "portMappings": [
        {
          "containerPort": 8080,
          "hostPort": 0,
          "labels": {
            "VIP_0": "/digitransit-ui-waltti:8080"
          },
          "protocol": "tcp",
          "servicePort": 10019
        }
      ],
      "privileged": false
    },
    "type": "DOCKER",
    "volumes": []
  },
  "cpus": 0.5,
  "dependencies": [],
  "disk": 0,
  "env": {
    "API_URL": "https://api.digitransit.fi",
    "MAP_URL": "https://digitransit-prod-cdn-origin.azureedge.net",
    "SENTRY_DSN": "sentrydsnenvsecret",
    "SENTRY_SECRET_DSN": "sentrydsnsecret",
    "PIWIK_ADDRESS": "//piwik.digitransit.fi/piwik.php",
    "NODE_OPTS": "--max_old_space_size=600",
    "RELAY_FETCH_TIMEOUT": "3000"
  },
  "gpus": 0,
  "healthChecks": [
    {
      "gracePeriodSeconds": 300,
      "ignoreHttp1xx": false,
      "intervalSeconds": 10,
      "maxConsecutiveFailures": 10,
      "path": "/",
      "portIndex": 0,
      "protocol": "HTTP",
      "timeoutSeconds": 10
    }
  ],
  "id": "/digitransit-ui-waltti",
  "instances": 4,
  "labels": {
    "restart-at": "03:10, 13:10",
    "restart-limit-interval": "480",
    "update": "auto"
  },
  "maxLaunchDelaySeconds": 3600,
  "mem": 1024,
  "portDefinitions": [
    {
      "labels": {},
      "port": 10019,
      "protocol": "tcp"
    }
  ],
  "ports": [
    10019
  ],
  "readinessChecks": [],
  "taskKillGracePeriodSeconds": null,
  "upgradeStrategy": {
    "maximumOverCapacity": 1,
    "minimumHealthCapacity": 1
  }
}
