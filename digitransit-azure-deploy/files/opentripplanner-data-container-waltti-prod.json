{
  "id": "/opentripplanner-data-con-waltti",
  "cmd": null,
  "cpus": 0.1,
  "mem": 128,
  "disk": 0,
  "instances": 2,
  "container": {
    "type": "DOCKER",
    "volumes": [],
    "docker": {
      "image": "hsldevcom/opentripplanner-data-container-waltti:prod",
      "network": "BRIDGE",
      "portMappings": [
        {
          "containerPort": 8080,
          "hostPort": 0,
          "servicePort": 10022,
          "protocol": "tcp",
          "labels": {
            "VIP_0": "/opentripplanner-data-con-waltti:8080"
          }
        }
      ],
      "privileged": false,
      "parameters": [
        {
          "key": "net",
          "value": "host"
        },
        {
          "key": "name",
          "value": "opentripplanner-data-con-waltti"
        }
      ],
      "forcePullImage": true
    }
  },
  "healthChecks": [
    {
      "path": "/version.txt",
      "protocol": "HTTP",
      "portIndex": 0,
      "gracePeriodSeconds": 300,
      "intervalSeconds": 10,
      "timeoutSeconds": 10,
      "maxConsecutiveFailures": 5,
      "ignoreHttp1xx": false
    }
  ],
  "labels": {
    "update": "auto"
  },
  "upgradeStrategy": {
    "minimumHealthCapacity": 1,
    "maximumOverCapacity": 1
  },
  "portDefinitions": [
    {
      "port": 10022,
      "protocol": "tcp",
      "labels": {}
    }
  ]
}
