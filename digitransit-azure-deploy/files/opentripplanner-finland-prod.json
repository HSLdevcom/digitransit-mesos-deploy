{
  "id": "/opentripplanner-finland",
  "cmd": null,
  "cpus": 6,
  "mem": 10240,
  "disk": 0,
  "instances": 2,
  "container": {
    "type": "DOCKER",
    "volumes": [],
    "docker": {
      "image": "hsldevcom/opentripplanner:prod",
      "network": "BRIDGE",
      "portMappings": [
        {
          "containerPort": 8080,
          "hostPort": 0,
          "servicePort": 10007,
          "protocol": "tcp",
          "name": "otpfinland",
          "labels": {
            "VIP_0": "/opentripplanner-finland:8080"
          }
        }
      ],
      "privileged": false,
      "parameters": [
        {
          "key": "net",
          "value": "host"
        },
        {
          "key": "name",
          "value": "opentripplanner-finland"
        }
      ],
      "forcePullImage": true
    }
  },
  "env": {
    "ROUTER_DATA_CONTAINER_URL": "http://opentripplanner-data-con.marathon.l4lb.thisdcos.directory:8080",
    "ROUTER_NAME": "finland",
    "JAVA_OPTS": "-Xmx7G -Xms7G -XX:+UseG1GC -XX:MaxGCPauseMillis=200 -XX:G1ReservePercent=10  -XX:+PrintGC"
  },
  "healthChecks": [
    {
      "path": "/",
      "protocol": "HTTP",
      "portIndex": 0,
      "gracePeriodSeconds": 2400,
      "intervalSeconds": 30,
      "timeoutSeconds": 20,
      "maxConsecutiveFailures": 10,
      "ignoreHttp1xx": false
    }
  ],
  "labels": {
    "update": "auto"
  },
  "portDefinitions": [
    {
      "port": 10007,
      "protocol": "tcp",
      "labels": {}
    }
  ]
}
