{
  "id": "/opentripplanner-waltti",
  "cmd": null,
  "cpus": 2,
  "mem": 7168,
  "disk": 0,
  "instances": 2,
  "container": {
    "type": "DOCKER",
    "volumes": [],
    "docker": {
      "image": "hsldevcom/opentripplanner",
      "network": "BRIDGE",
      "portMappings": [
        {
          "containerPort": 8080,
          "hostPort": 0,
          "servicePort": 10008,
          "protocol": "tcp",
          "name": "otpfinland",
          "labels": {
            "VIP_0": "/opentripplanner-waltti:8080"
          }
        }
      ],
      "privileged": false,
      "parameters": [
        {
          "key": "net",
          "value": "host"
        },
        {
          "key": "name",
          "value": "opentripplanner-waltti"
        }
      ],
      "forcePullImage": true
    }
  },
  "env": {
    "ROUTER_DATA_CONTAINER_URL": "http://opentripplanner-data-con.marathon.l4lb.thisdcos.directory:8080",
    "ROUTER_NAME": "waltti",
    "JAVA_OPTS": "-Xmx6G -Xms6G"
  },
  "healthChecks": [
    {
      "path": "/",
      "protocol": "HTTP",
      "portIndex": 0,
      "gracePeriodSeconds": 1200,
      "intervalSeconds": 10,
      "timeoutSeconds": 30,
      "maxConsecutiveFailures": 10,
      "ignoreHttp1xx": false
    }
  ],
  "labels": {
    "update": "auto"
  },
  "portDefinitions": [
    {
      "port": 10008,
      "protocol": "tcp",
      "labels": {}
    }
  ],
"upgradeStrategy": {
    "minimumHealthCapacity": 0.5,
    "maximumOverCapacity": 1
  }
}
