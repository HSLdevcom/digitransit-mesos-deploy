{
  "acceptedResourceRoles": null,
  "backoffFactor": 1.15,
  "backoffSeconds": 1,
  "cmd": null,
  "constraints": [],
  "container": {
      "docker": {
          "forcePullImage": true,
          "image": "hsldevcom/opentripplanner",
          "network": "BRIDGE",
          "parameters": [
              {
                  "key": "net",
                  "value": "host"
              },
              {
                  "key": "name",
                  "value": "opentripplanner-waltti"
              }
          ],
          "portMappings": [
              {
                  "containerPort": 8080,
                  "hostPort": 0,
                  "labels": {
                      "VIP_0": "/opentripplanner-waltti:8080"
                  },
                  "name": "otpfinland",
                  "protocol": "tcp",
                  "servicePort": 10008
              }
          ],
          "privileged": false
      },
      "type": "DOCKER",
      "volumes": []
  },
  "cpus": 1,
  "dependencies": [],
  "disk": 0,
  "env": {
      "ROUTER_DATA_CONTAINER_URL": "https://dev-api.digitransit.fi/routing-data/v2/waltti",
      "ROUTER_NAME": "waltti",
      "JAVA_OPTS": "-Dsentry.dsn=sentry_routing_secret_dsn -XX:+UseParNewGC -Xms7G -Xmx7G -XX:NewRatio=5 -XX:+UseStringDeduplication"
  },
  "gpus": 0,
  "healthChecks": [
      {
          "command": {
              "value": "test $(curl -4 -s -o /dev/null -w \"%{http_code}\" http://localhost:$PORT0/otp/routers/waltti/) = 200"
          },
          "gracePeriodSeconds": 6000,
          "ignoreHttp1xx": false,
          "intervalSeconds": 30,
          "maxConsecutiveFailures": 6,
          "protocol": "COMMAND",
          "timeoutSeconds": 20
      }
  ],
  "id": "/opentripplanner-waltti",
  "instances": 1,
  "labels": {
      "restart-after-services": "/opentripplanner-data-con-waltti",
      "restart-delay": "10",
      "update": "auto"
  },
  "maxLaunchDelaySeconds": 3600,
  "mem": 10240,
  "portDefinitions": [
      {
          "labels": {},
          "port": 10008,
          "protocol": "tcp"
      }
  ],
  "ports": [
      10008
  ],
  "readinessChecks": [],
  "taskKillGracePeriodSeconds": null,
  "upgradeStrategy": {
      "maximumOverCapacity": 1,
      "minimumHealthCapacity": 0.5
  }
}
