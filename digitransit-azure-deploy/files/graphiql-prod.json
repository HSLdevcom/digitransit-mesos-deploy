{
  "id": "/graphiql",
  "cmd": null,
  "cpus": 1,
  "mem": 128,
  "disk": 0,
  "instances": 1,
  "container": {
    "type": "DOCKER",
    "volumes": [],
    "docker": {
      "image": "hsldevcom/graphiql",
      "network": "BRIDGE",
      "portMappings": [
        {
          "containerPort": 8080,
          "hostPort": 0,
          "servicePort": 10025,
          "protocol": "tcp",
          "labels": {
            "VIP_0": "/graphiql:8080"
          }
        }
      ],
      "privileged": false,
      "parameters": [
        {
          "key": "name",
          "value": "graphiql"
        }
      ],
      "forcePullImage": true
    }
  },
  "env": {
    "URL_PREFIX": "https://api.digitransit.fi"
  },
  "healthChecks": [
    {
      "path": "/graphiql/hsl",
      "protocol": "HTTP",
      "portIndex": 0,
      "gracePeriodSeconds": 60,
      "intervalSeconds": 60,
      "timeoutSeconds": 10,
      "maxConsecutiveFailures": 1,
      "ignoreHttp1xx": false
    }
  ],
  "labels": {
    "update": "auto"
  },
  "portDefinitions": [
    {
      "port": 10025,
      "protocol": "tcp",
      "labels": {}
    }
  ]
}
