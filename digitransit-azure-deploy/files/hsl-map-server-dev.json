{
  "acceptedResourceRoles": null,
  "backoffFactor": 1.15,
  "backoffSeconds": 1,
  "cmd": null,
  "container": {
      "docker": {
          "forcePullImage": true,
          "image": "hsldevcom/hsl-map-server",
          "network": "BRIDGE",
          "parameters": [
              {
                  "key": "net",
                  "value": "host"
              }
          ],
          "portMappings": [
              {
                  "containerPort": 8080,
                  "hostPort": 0,
                  "labels": {
                      "VIP_0": "/hsl-map-server:8080"
                  },
                  "name": "8080",
                  "protocol": "tcp",
                  "servicePort": 10018
              }
          ],
          "privileged": false
      },
      "type": "DOCKER",
      "volumes": []
  },
  "cpus": 0.2,
  "dependencies": [],
  "disk": 0,
  "env": {
      "FONTSTACK_PASSWORD": "fontstacksecret",
      "HSL_OTP_URL": "opentripplanner-hsl:8080/otp/routers/hsl/index/graphql",
      "FINLAND_OTP_URL": "opentripplanner-finland:8080/otp/routers/finland/index/graphql",
      "WALTTI_OTP_URL": "opentripplanner-waltti:8080/otp/routers/waltti/index/graphql",
      "NODE_OPTS": "--max_old_space_size=2048 --optimize_for_size --max_executable_size=2048 --stack_size=2048 --harmony"
  },
  "gpus": 0,
  "healthChecks": [
      {
          "gracePeriodSeconds": 300,
          "ignoreHttp1xx": false,
          "intervalSeconds": 10,
          "maxConsecutiveFailures": 10,
          "path": "/map/v1/hsl-map/index.json",
          "portIndex": 0,
          "protocol": "HTTP",
          "timeoutSeconds": 10
      }
  ],
  "id": "/hsl-map-server",
  "instances": 2,
  "labels": {
      "restart-after-services": "/opentripplanner-finland,/opentripplanner-hsl,/opentripplanner-waltti",
      "restart-delay": "5",
      "update": "auto"
  },
  "maxLaunchDelaySeconds": 3600,
  "mem": 3584,
  "portDefinitions": [
      {
          "labels": {},
          "port": 10018,
          "protocol": "tcp"
      }
  ],
  "ports": [
      10018
  ],
  "readinessChecks": [],
  "taskKillGracePeriodSeconds": null,
  "upgradeStrategy": {
      "maximumOverCapacity": 1,
      "minimumHealthCapacity": 0
  }
}
