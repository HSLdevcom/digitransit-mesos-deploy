{
  "acceptedResourceRoles": [
    "slave_public",
    "*"
  ],
  "backoffFactor": 1.15,
  "backoffSeconds": 1,
  "cmd": "sh -c \"while true; do ./docker-gc ; sleep 86400; done\"\n",
  "constraints": [
    [
        "hostname",
        "UNIQUE"
    ]
  ],
  "container": {
    "docker": {
      "forcePullImage": false,
      "image": "hsldevcom/docker-gc:1.11.2_0.1.1",
      "network": "HOST",
      "parameters": [],
      "portMappings": null,
      "privileged": false
    },
    "type": "DOCKER",
    "volumes": [
      {
        "containerPath": "/var/run/docker.sock",
        "hostPath": "/var/run/docker.sock",
        "mode": "RW"
      },
      {
        "containerPath": "/etc",
        "hostPath": "/etc",
        "mode": "RO"
      }
    ]
  },
  "cpus": 0.01,
  "dependencies": [],
  "disk": 0,
  "env": {},
  "gpus": 0,
  "healthChecks": [
    {
      "command": {
        "value": "echo \"OK\""
      },
      "gracePeriodSeconds": 300,
      "ignoreHttp1xx": false,
      "intervalSeconds": 60,
      "maxConsecutiveFailures": 3,
      "protocol": "COMMAND",
      "timeoutSeconds": 20
    }
  ],
  "id": "/digitransit-docker-gc",
  "instances": 60,
  "labels": {},
  "maxLaunchDelaySeconds": 3600,
  "mem": 32,
  "portDefinitions": [
    {
      "labels": {},
      "port": 10009,
      "protocol": "tcp"
    }
  ],
  "ports": [
    10009
  ],
  "readinessChecks": [],
  "taskKillGracePeriodSeconds": null,
  "upgradeStrategy": {
    "maximumOverCapacity": 1,
    "minimumHealthCapacity": 1
  }
}