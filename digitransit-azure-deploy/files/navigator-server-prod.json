{
  "id": "/navigator-server",
  "cmd": null,
  "cpus": 1,
  "mem": 500,
  "disk": 0,
  "instances": 2,
  "container": {
    "type": "DOCKER",
    "volumes": [],
    "docker": {
      "image": "hsldevcom/navigator-server:prod",
      "network": "BRIDGE",
      "portMappings": [
        {
          "containerPort": 8080,
          "hostPort": 0,
          "servicePort": 10013,
          "protocol": "tcp",
          "labels": {
            "VIP_0": "/navigator-server:8080"
          }
        }
      ],
      "privileged": false,
      "parameters": [
        {
          "key": "net",
          "value": "host"
        },
        {
          "key": "name",
          "value": "navigator-server"
        }
      ],
      "forcePullImage": true
    }
  },
  "env": {
    "NODE_OPTS": "--max_old_space_size=400",
  },
  "healthChecks": [
    {
      "path": "/",
      "protocol": "HTTP",
      "portIndex": 0,
      "gracePeriodSeconds": 300,
      "intervalSeconds": 30,
      "timeoutSeconds": 20,
      "maxConsecutiveFailures": 5,
      "ignoreHttp1xx": false
    }
  ],
  "labels": {
    "update": "auto"
  },
  "upgradeStrategy": {
    "minimumHealthCapacity": 1,
    "maximumOverCapacity": 1
  },
  "portDefinitions": [
    {
      "port": 10013,
      "protocol": "tcp",
      "labels": {}
    }
  ]
}
