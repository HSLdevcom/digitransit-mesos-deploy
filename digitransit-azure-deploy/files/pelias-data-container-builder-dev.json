{
  "acceptedResourceRoles": null,
  "backoffFactor": 1.15,
  "backoffSeconds": 1,
  "cmd": null,
  "constraints": [],
  "container": {
      "docker": {
          "forcePullImage": true,
          "image": "hsldevcom/pelias-data-container-builder",
          "network": "HOST",
          "parameters": [],
          "portMappings": null,
          "privileged": false
      },
      "type": "DOCKER",
      "volumes": [
          {
              "containerPath": "/var/run/docker.sock",
              "hostPath": "/var/run/docker.sock",
              "mode": "RO"
          },
          {
              "containerPath": "persistent",
              "mode": "RW",
              "persistent": {
                  "size": 5
              }
          }
      ]
  },
  "cpus": 2,
  "dependencies": [],
  "disk": 0,
  "env": {
      "DOCKER_AUTH": "dockerauth",
      "DOCKER_USER": "dockeruser",
      "SLACK_WEBHOOK_URL": "slackwebhook",
      "MMLAPIKEY": "mmlapikey"
  },
  "gpus": 0,
  "healthChecks": [
      {
          "command": {
              "value": "true"
          },
          "gracePeriodSeconds": 30,
          "ignoreHttp1xx": false,
          "intervalSeconds": 60,
          "maxConsecutiveFailures": 2,
          "protocol": "COMMAND",
          "timeoutSeconds": 20
      }
  ],
  "id": "/pelias-data-container-builder",
  "instances": 1,
  "labels": {
      "update": "auto"
  },
  "maxLaunchDelaySeconds": 3600,
  "mem": 6000,
  "portDefinitions": [
      {
          "labels": {},
          "port": 10023,
          "protocol": "tcp"
      }
  ],
  "ports": [
      10023
  ],
  "readinessChecks": [],
  "taskKillGracePeriodSeconds": null,
  "upgradeStrategy": {
      "maximumOverCapacity": 0,
      "minimumHealthCapacity": 0
  }
}
