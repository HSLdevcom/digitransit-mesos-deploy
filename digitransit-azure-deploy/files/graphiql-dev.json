{
  "acceptedResourceRoles": null,
  "backoffFactor": 1.15,
  "backoffSeconds": 1,
  "cmd": null,
  "constraints": [
        [
            "hostname",
            "UNIQUE"
        ]
    ],
  "container": {
      "docker": {
          "forcePullImage": true,
          "image": "hsldevcom/graphiql",
          "network": "BRIDGE",
          "parameters": [
              {
                  "key": "name",
                  "value": "graphiql"
              }
          ],
          "portMappings": [
              {
                  "containerPort": 8080,
                  "hostPort": 0,
                  "labels": {
                      "VIP_0": "/graphiql:8080"
                  },
                  "protocol": "tcp",
                  "servicePort": 10025
              }
          ],
          "privileged": false
      },
      "type": "DOCKER",
      "volumes": []
  },
  "cpus": 1,
  "dependencies": [],
  "disk": 0,
  "env": {},
  "gpus": 0,
  "healthChecks": [
      {
          "gracePeriodSeconds": 60,
          "ignoreHttp1xx": false,
          "intervalSeconds": 60,
          "maxConsecutiveFailures": 1,
          "path": "/graphiql/hsl",
          "portIndex": 0,
          "protocol": "HTTP",
          "timeoutSeconds": 10
      }
  ],
  "id": "/graphiql",
  "instances": 1,
  "labels": {
      "update": "auto"
  },
  "maxLaunchDelaySeconds": 3600,
  "mem": 64,
  "portDefinitions": [
      {
          "labels": {},
          "port": 10025,
          "protocol": "tcp"
      }
  ],
  "ports": [
      10025
  ],
  "readinessChecks": [],
  "taskKillGracePeriodSeconds": null,
  "upgradeStrategy": {
      "maximumOverCapacity": 1,
      "minimumHealthCapacity": 1
  }
}
