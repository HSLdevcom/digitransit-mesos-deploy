{
  "acceptedResourceRoles": null,
  "backoffFactor": 1.15,
  "backoffSeconds": 1,
  "cmd": null,
  "constraints": [
    [
      "hostname",
      "UNIQUE"
    ]
  ],
  "container": {
    "docker": {
      "forcePullImage": true,
      "image": "hsldevcom/siri2gtfsrt:prod",
      "network": "BRIDGE",
      "parameters": [
        {
          "key": "net",
          "value": "host"
        },
        {
          "key": "name",
          "value": "siri2gtfsrt"
        }
      ],
      "portMappings": [
        {
          "containerPort": 8080,
          "hostPort": 0,
          "labels": {
            "VIP_0": "/siri2gtfsrt:8080"
          },
          "protocol": "tcp",
          "servicePort": 10016
        }
      ],
      "privileged": false
    },
    "type": "DOCKER",
    "volumes": []
  },
  "cpus": 0.3,
  "dependencies": [],
  "disk": 0,
  "env": {
    "HSL_URL": "http://navigator-server.marathon.l4lb.thisdcos.directory:8080/siriaccess/vm/json?operatorRef=HSL",
    "TRAIN_URL": "http://raildigitraffic2gtfsrt.marathon.l4lb.thisdcos.directory:8080/hsl",
    "TRIP_UPDATE_URL": "http://hslalert.marathon.l4lb.thisdcos.directory:8080/"
  },
  "gpus": 0,
  "healthChecks": [
    {
      "gracePeriodSeconds": 300,
      "ignoreHttp1xx": false,
      "intervalSeconds": 10,
      "maxConsecutiveFailures": 5,
      "path": "/HSL",
      "portIndex": 0,
      "protocol": "HTTP",
      "timeoutSeconds": 10
    }
  ],
  "id": "/siri2gtfsrt",
  "instances": 2,
  "labels": {
    "update": "auto"
  },
  "maxLaunchDelaySeconds": 3600,
  "mem": 1000,
  "portDefinitions": [
    {
      "labels": {},
      "port": 10016,
      "protocol": "tcp"
    }
  ],
  "ports": [
    10016
  ],
  "readinessChecks": [],
  "taskKillGracePeriodSeconds": null,
  "upgradeStrategy": {
    "maximumOverCapacity": 1,
    "minimumHealthCapacity": 1
  }
}
