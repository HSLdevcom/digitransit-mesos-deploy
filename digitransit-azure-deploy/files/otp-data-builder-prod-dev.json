{
    "acceptedResourceRoles": null,
    "backoffFactor": 1.15,
    "backoffSeconds": 1,
    "cmd": null,
    "container": {
        "docker": {
            "forcePullImage": true,
            "image": "hsldevcom/otp-data-builder",
            "network": "HOST",
            "parameters": [],
            "portMappings": null,
            "privileged": false
        },
        "type": "DOCKER",
        "volumes": [
            {
                "containerPath": "/var/run/docker.sock",
                "hostPath": "/var/run/docker.sock",
                "mode": "RO"
            },
            {
                "containerPath": "/opt/otp-data-builder/data",
                "hostPath": "/tmp/data",
                "mode": "RW"
            },
            {
                "containerPath": "persistent",
                "mode": "RW",
                "persistent": {
                    "size": 5
                }
            }
        ]
    },
    "cpus": 3,
    "dependencies": [],
    "disk": 0,
    "env": {
        "HOST_DATA": "/tmp/data",
        "DATA": "/opt/otp-data-builder/data",
        "DOCKER_AUTH": "dockerauth",
        "SLACK_WEBHOOK_URL": "slackwebhook",
        "BUILDER_TYPE": "prod",
        "DOCKER_USER": "dockeruser",
        "CRON": "0 0 3 * * *",
        "SEED_TAG": "prod",
        "OTP_TAG": "prod",
        "DOCKER_TAG": "prod"
    },
    "gpus": 0,
    "healthChecks": [
        {
            "command": {
                "value": "true"
            },
            "gracePeriodSeconds": 30,
            "ignoreHttp1xx": false,
            "intervalSeconds": 60,
            "maxConsecutiveFailures": 2,
            "protocol": "COMMAND",
            "timeoutSeconds": 20
        }
    ],
    "id": "/otp-data-builder-prod",
    "instances": 1,
    "labels": {
        "update": "auto"
    },
    "maxLaunchDelaySeconds": 3600,
    "mem": 9000,
    "portDefinitions": [
        {
            "labels": {},
            "port": 10028,
            "protocol": "tcp"
        }
    ],
    "ports": [
        10028
    ],
    "readinessChecks": [],
    "taskKillGracePeriodSeconds": null,
    "upgradeStrategy": {
        "maximumOverCapacity": 0,
        "minimumHealthCapacity": 0.5
    }
  }
