{
    "acceptedResourceRoles": null,
    "backoffFactor": 1.15,
    "backoffSeconds": 1,
    "cmd": null,
    "constraints": [],
    "container": {
        "docker": {
            "forcePullImage": true,
            "image": "hsldevcom/otp-data-builder",
            "network": "HOST",
            "parameters": [],
            "portMappings": null,
            "privileged": false
        },
        "type": "DOCKER",
        "volumes": [
            {
                "containerPath": "/var/run/docker.sock",
                "hostPath": "/var/run/docker.sock",
                "mode": "RO"
            },
            {
                "containerPath": "/opt/otp-data-builder/data",
                "hostPath": "/var/lib/docker/otp/data",
                "mode": "RW"
            },
            {
                "containerPath": "persistent",
                "mode": "RW",
                "persistent": {
                    "size": 5
                }
            }
        ]
    },
    "cpus": 3,
    "dependencies": [],
    "disk": 0,
    "env": {
        "BUILDER_TYPE": "dev",
        "BUILD_TIME": "23:00:00",
        "DATA": "/opt/otp-data-builder/data",
        "DOCKER_AUTH": "dockerauth",
        "DOCKER_TAG": "latest",
        "DOCKER_USER": "dockeruser",
        "EXTRA_SRC": "{\"tampere\": {\"url\": \"http://www.tampere.fi/ekstrat/ptdata/tamperefeed_faret.zip\", \"routers\": [\"waltti\", \"finland\"]}}",
        "EXTRA_UPDATERS": "{\"foli-alerts\": {\"url\": \"https://foli-beta.nanona.fi/gtfs-rt/reittiopas\", \"routers\": [\"waltti\"]}, \"hsl-trip-updates\": {\"topic\": \"gtfsrt/dev/fi/hsl/tu\", \"routers\": [\"hsl\", \"finland\"]}, \"hsl-alerts\": {\"url\": \"https://dev-api.digitransit.fi/realtime/service-alerts/v2/hsl\", \"routers\": [\"hsl\", \"finland\"]}}",
        "HOST_DATA": "/var/lib/docker/otp/data",
        "OTP_TAG": "latest",
        "ROUTERS": "finland, waltti, hsl",
        "SEED_TAG": "latest",
        "SLACK_WEBHOOK_URL": "slackwebhook",
        "TOOLS_TAG": "latest"
    },
    "gpus": 0,
    "healthChecks": [
        {
            "command": {
                "value": "true"
            },
            "gracePeriodSeconds": 30,
            "ignoreHttp1xx": false,
            "intervalSeconds": 60,
            "maxConsecutiveFailures": 2,
            "protocol": "COMMAND",
            "timeoutSeconds": 20
        }
    ],
    "id": "/otp-data-builder-dev",
    "instances": 1,
    "labels": {
        "update": "auto"
    },
    "maxLaunchDelaySeconds": 3600,
    "mem": 12000,
    "portDefinitions": [
        {
            "labels": {},
            "port": 10027,
            "protocol": "tcp"
        }
    ],
    "ports": [
        10027
    ],
    "readinessChecks": [],
    "taskKillGracePeriodSeconds": null,
    "upgradeStrategy": {
        "maximumOverCapacity": 0,
        "minimumHealthCapacity": 0.5
    }
}