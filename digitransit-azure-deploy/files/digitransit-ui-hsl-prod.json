{
  "id": "/digitransit-ui-hsl",
  "cmd": null,
  "cpus": 1,
  "mem": 1024,
  "disk": 0,
  "instances": 8,
  "container": {
    "type": "DOCKER",
    "volumes": [],
    "docker": {
      "image": "hsldevcom/digitransit-ui:prod",
      "network": "BRIDGE",
      "portMappings": [
        {
          "containerPort": 8080,
          "hostPort": 0,
          "servicePort": 10001,
          "protocol": "tcp",
          "labels": {
            "VIP_0": "/digitransit-ui-hsl:8080"
          }
        }
      ],
      "privileged": false,
      "parameters": [
        {
          "key": "net",
          "value": "host"
        }
      ],
      "forcePullImage": true
    }
  },
  "env": {
    "CONFIG": "hsl",
    "API_URL": "https://api.digitransit.fi",
    "MAP_URL": "https://digitransit-prod-cdn-origin.azureedge.net",
    "SENTRY_DSN": "sentrydsnenvsecret",
    "SENTRY_SECRET_DSN": "sentrydsnsecret",
    "PIWIK_ID": "4",
    "PIWIK_ADDRESS": "//piwik.digitransit.fi/piwik.php",
    "NODE_OPTS": "--max_old_space_size=600",
    "RELAY_FETCH_TIMEOUT": "3000"
  },
  "healthChecks": [
    {
      "path": "/",
      "protocol": "HTTP",
      "portIndex": 0,
      "gracePeriodSeconds": 300,
      "intervalSeconds": 10,
      "timeoutSeconds": 10,
      "maxConsecutiveFailures": 10,
      "ignoreHttp1xx": false
    }
  ],
  "labels": {
    "update": "auto"
  },
  "portDefinitions": [
    {
      "port": 10001,
      "protocol": "tcp",
      "labels": {}
    }
  ]
}
