{
  "acceptedResourceRoles": null,
  "backoffFactor": 1.15,
  "backoffSeconds": 1,
  "cmd": null,
  "container": {
      "docker": {
          "forcePullImage": true,
          "image": "hsldevcom/hslalert",
          "network": "BRIDGE",
          "parameters": [
              {
                  "key": "net",
                  "value": "host"
              },
              {
                  "key": "name",
                  "value": "hslalert"
              }
          ],
          "portMappings": [
              {
                  "containerPort": 8080,
                  "hostPort": 0,
                  "labels": {
                      "VIP_0": "/hslalert:8080"
                  },
                  "protocol": "tcp",
                  "servicePort": 10006
              }
          ],
          "privileged": false
      },
      "type": "DOCKER",
      "volumes": []
  },
  "cpus": 0.1,
  "dependencies": [],
  "disk": 0,
  "env": {},
  "gpus": 0,
  "healthChecks": [
      {
          "gracePeriodSeconds": 300,
          "ignoreHttp1xx": false,
          "intervalSeconds": 10,
          "maxConsecutiveFailures": 5,
          "path": "/",
          "portIndex": 0,
          "protocol": "HTTP",
          "timeoutSeconds": 10
      }
  ],
  "id": "/hslalert",
  "instances": 2,
  "labels": {
      "update": "auto"
  },
  "maxLaunchDelaySeconds": 3600,
  "mem": 128,
  "portDefinitions": [
      {
          "labels": {},
          "port": 10006,
          "protocol": "tcp"
      }
  ],
  "ports": [
      10006
  ],
  "readinessChecks": [],
  "taskKillGracePeriodSeconds": null,
  "upgradeStrategy": {
      "maximumOverCapacity": 1,
      "minimumHealthCapacity": 0
  }
}
