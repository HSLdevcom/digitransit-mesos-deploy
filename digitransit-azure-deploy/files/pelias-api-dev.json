{
  "id": "/pelias-api",
  "cmd": null,
  "cpus": 1,
  "mem": 4096,
  "disk": 0,
  "instances": 2,
  "container": {
    "type": "DOCKER",
    "volumes": [],
    "docker": {
      "image": "hsldevcom/pelias-api",
      "network": "BRIDGE",
      "portMappings": [
        {
          "containerPort": 8080,
          "hostPort": 0,
          "servicePort": 10005,
          "protocol": "tcp",
          "labels": {
            "VIP_0": "/pelias-api:8080"
          }
        }
      ],
      "privileged": false,
      "parameters": [
        {
          "key": "net",
          "value": "host"
        },
        {
          "key": "name",
          "value": "pelias-api"
        }
      ],
      "forcePullImage": true
    }
  },
  "healthChecks": [
    {
      "path": "/v1/search?text=helsinki",
      "protocol": "HTTP",
      "portIndex": 0,
      "gracePeriodSeconds": 300,
      "intervalSeconds": 10,
      "timeoutSeconds": 10,
      "maxConsecutiveFailures": 5,
      "ignoreHttp1xx": false
    }
  ],
  "labels": {
    "update": "auto"
  },
  "portDefinitions": [
    {
      "port": 10005,
      "protocol": "tcp",
      "labels": {}
    }
  ]
}
