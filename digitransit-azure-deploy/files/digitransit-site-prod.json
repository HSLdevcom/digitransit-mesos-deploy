{
  "acceptedResourceRoles": null,
  "backoffFactor": 1.15,
  "backoffSeconds": 1,
  "cmd": null,
  "container": {
    "docker": {
      "forcePullImage": true,
      "image": "hsldevcom/digitransit-site:prod",
      "network": "BRIDGE",
      "parameters": [
        {
          "key": "name",
          "value": "digitransit-site"
        }
      ],
      "portMappings": [
        {
          "containerPort": 8080,
          "hostPort": 0,
          "labels": {
            "VIP_0": "/digitransit-site:8080"
          },
          "protocol": "tcp",
          "servicePort": 10012
        }
      ],
      "privileged": false
    },
    "type": "DOCKER",
    "volumes": []
  },
  "cpus": 0.1,
  "dependencies": [],
  "disk": 0,
  "env": {},
  "gpus": 0,
  "healthChecks": [
    {
      "gracePeriodSeconds": 300,
      "ignoreHttp1xx": false,
      "intervalSeconds": 10,
      "maxConsecutiveFailures": 5,
      "path": "/",
      "portIndex": 0,
      "protocol": "HTTP",
      "timeoutSeconds": 10
    }
  ],
  "id": "/digitransit-site",
  "instances": 2,
  "labels": {
    "update": "auto"
  },
  "maxLaunchDelaySeconds": 3600,
  "mem": 256,
  "portDefinitions": [
    {
      "labels": {},
      "port": 10012,
      "protocol": "tcp"
    }
  ],
  "ports": [
    10012
  ],
  "readinessChecks": [],
  "taskKillGracePeriodSeconds": null,
  "upgradeStrategy": {
    "maximumOverCapacity": 1,
    "minimumHealthCapacity": 1
  }
}
