    "MATKAHUOLTO_KARJALA_BASIC_AUTH": "\"Basic matkahuoltokarjalabasickey\"",
    "MATKAHUOLTO_POHJANMAA_BASIC_AUTH": "\"Basic matkahuoltopohjanmaabasickey\"",
    "MATKAHUOLTO_SAVO_BASIC_AUTH": "\"Basic matkahuoltosavobasickey\"",
