  "cpus": 2,
  "dependencies": [],
  "disk": 0,
  "env": {
    "SAMOCAT_TOKEN_AUTH": "\"Token samocattokenkey\"",
    "JOJO_BASIC_AUTH": "\"Basic jojobasickey\"",
    "LINKKI_BASIC_AUTH": "\"Basic linkkibasickey\"",
    "VILKKU_BASIC_AUTH": "\"Basic vilkkubasickey\"",
