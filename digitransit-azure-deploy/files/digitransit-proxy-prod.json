    "MATKAHUOLTO_LAPPI_BASIC_AUTH": "\"Basic matkahuoltolappibasickey\"",
    "MATKAHUOLTO_VANTAA_BASIC_AUTH": "\"Basic matkahuoltovantaabasickey\"",
