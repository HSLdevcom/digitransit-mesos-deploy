{
    "acceptedResourceRoles": null,
    "backoffFactor": 1.15,
    "backoffSeconds": 1,
    "cmd": null,
    "constraints": [
        [
             "hostname",
             "UNIQUE"
        ]
    ],
    "container": {
        "docker": {
            "forcePullImage": true,
            "image": "hsldevcom/opentripplanner:latest",
            "network": "BRIDGE",
            "parameters": [
                {
                    "key": "net",
                    "value": "host"
                },
                {
                    "key": "name",
                    "value": "opentripplanner-next"
                }
            ],
            "portMappings": [
                {
                    "containerPort": 8080,
                    "hostPort": 0,
                    "labels": {
                        "VIP_0": "/opentripplanner-next:8080"
                    },
                    "name": "otphsl",
                    "protocol": "tcp",
                    "servicePort": 10029
                }
            ],
            "privileged": false
        },
        "type": "DOCKER",
        "volumes": []
    },
    "cpus": 1,
    "dependencies": [],
    "disk": 0,
    "env": {
        "JAVA_OPTS": "-Dsentry.dsn=sentry_routing_secret_dsn -Xmx10G -Xms10G -XX:+UseG1GC -XX:MaxGCPauseMillis=200 -XX:G1ReservePercent=10",
        "ROUTER_DATA_CONTAINER_URL": "https://dev-api.digitransit.fi/routing-data/v2/next-hsl",
        "ROUTER_NAME": "hsl"
    },
    "gpus": 0,
    "healthChecks": [
        {
            "command": {
                "value": "test $(curl -4 -s -o /dev/null -w \"%{http_code}\" http://localhost:$PORT0/otp/routers/hsl/) = 200"
            },
            "gracePeriodSeconds": 8000,
            "ignoreHttp1xx": false,
            "intervalSeconds": 30,
            "maxConsecutiveFailures": 6,
            "protocol": "COMMAND",
            "timeoutSeconds": 20
        }
    ],
    "id": "/opentripplanner-next",
    "instances": 1,
    "labels": {
        "restart-after-services": "/opentripplanner-data-con-next-hsl",
        "restart-delay": "1",
        "update": "auto"
    },
    "maxLaunchDelaySeconds": 3600,
    "mem": 12288,
    "portDefinitions": [
        {
            "labels": {},
            "port": 10029,
            "protocol": "tcp"
        }
    ],
    "ports": [
        10029
    ],
    "readinessChecks": [],
    "taskKillGracePeriodSeconds": null,
    "upgradeStrategy": {
        "maximumOverCapacity": 1,
        "minimumHealthCapacity": 0
    }
}