{
  "id": "/raildigitraffic2gtfsrt",
  "cmd": null,
  "cpus": 0.1,
  "mem": 2048,
  "disk": 0,
  "instances": 2,
  "container": {
    "type": "DOCKER",
    "volumes": [],
    "docker": {
      "image": "hsldevcom/raildigitraffic2gtfsrt",
      "network": "BRIDGE",
      "portMappings": [
        {
          "containerPort": 8080,
          "hostPort": 0,
          "servicePort": 10015,
          "protocol": "tcp",
          "labels": {
            "VIP_0": "/raildigitraffic2gtfsrt:8080"
          }
        }
      ],
      "privileged": false,
      "parameters": [
        {
          "key": "net",
          "value": "host"
        },
        {
          "key": "name",
          "value": "raildigitraffic2gtfsrt"
        }
      ],
      "forcePullImage": true
    }
  },
  "env": {
    "ROUTER_ZIP_URL": "https://dev-api.digitransit.fi/routing-data/v2/finland/router-finland.zip"
  },
  "healthChecks": [
    {
      "path": "/hsl",
      "protocol": "HTTP",
      "portIndex": 0,
      "gracePeriodSeconds": 300,
      "intervalSeconds": 10,
      "timeoutSeconds": 10,
      "maxConsecutiveFailures": 5,
      "ignoreHttp1xx": false
    }
  ],
  "labels": {
    "update": "auto",
    "restart-after-services": "/opentripplanner-data-con-finland",
    "restart-delay": "5"
  },
  "upgradeStrategy": {
    "minimumHealthCapacity": 0,
    "maximumOverCapacity": 1
  },
  "portDefinitions": [
    {
      "port": 10015,
      "protocol": "tcp",
      "labels": {}
    }
  ]
}
